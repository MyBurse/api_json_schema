{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Метод для получения списка категорий",
  "properties": {
    "q": {
      "type": "string",
      "description": "Поисковая строка, используется для поиска категории, если не задан id"
    },
    "last_id": {
      "type": "string",
      "description": "Последний идентификатор"
    },
    "limit": {
      "type": "integer",
      "description": "Лимит"
    }
  },
  "required": [
    "q"
  ],
  "type": "object"
}