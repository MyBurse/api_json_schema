{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Method to get all feed", 
    "properties": {
        "filters": {
            "description": "Filters", 
            "properties": {
                "distance": {
                    "description": "Distance to find", 
                    "minNum": 1, 
                    "type": "integer"
                }, 
                "item_type": {
                    "description": "Item types", 
                    "enum": [
                        "product", 
                        "blog_post", 
                        "wall_post", 
                        "all"
                    ], 
                    "minNum": 1, 
                    "type": "string"
                }, 
                "latitude": {
                    "description": "Latitude", 
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ]
                }, 
                "longitude": {
                    "description": "Longitude", 
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ]
                }, 
                "product_type": {
                    "description": "Product types", 
                    "enum": [
                        "buynow", 
                        "advert", 
                        "classic", 
                        "quick", 
                        "penny", 
                        "tender", 
                        "all", 
                        "null"
                    ], 
                    "minNum": 1, 
                    "type": "string"
                }
            }, 
            "type": "object"
        }, 
        "last_id": {
            "description": "Last ID",
            "type": "string"
        }, 
        "limit": {
            "default": 20, 
            "description": "Limit", 
            "minNum": 1, 
            "type": "integer"
        }
    }, 
    "required": [], 
    "type": "object"
}