{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Метод для получения списка магазинов",
    "properties": {
        "filters": {
            "description": "Фильтры",
            "properties": {
                "geo": {
                    "description": "Фильтр по геолокации: GeoType_GeoId",
                    "type": "string"
                },
                "latitude": {
                    "description": "Широта",
                    "minNum": 1,
                    "type": [
                        "number",
                        "null"
                    ]
                },
                "longitude": {
                    "description": "Долгота",
                    "minNum": 1,
                    "type": [
                        "number",
                        "null"
                    ]
                },
                "q": {
                    "description": "Поисковый запрос",
                    "type": "string"
                },
                "tab": {
                    "description": "Активная вкладка",
                    "enum": [
                        "all",
                        "my",
                        "subscribe",
                        "interest"
                    ],
                    "type": "string"
                },
                "action_type": {
                    "description": "Типы продаж",
                    "enum": [
                        "sell",
                        "buy"
                    ],
                    "minNum": 1,
                    "type": "string"
                }
            },
            "type": "object"
        },
        "last_id": {
            "description": "Последний идентификатор",
            "type": "string"
        },
        "limit": {
            "description": "Limit",
            "minNum": 1,
            "type": "integer"
        }
    },
    "type": "object"
}