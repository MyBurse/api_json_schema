{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Метод для получения списка лайков",
    "method": "post",
    "properties": {
        "id": {
            "type": "integer"
        },
        "type": {
            "type": "string",
            "enum": [
                "product",
                "wall_post",
                "blog_post",
                "image",
                "comment"
            ]
        }
    }, 
    "required": [
        "id",
        "type"
    ],
    "type": "object"
}