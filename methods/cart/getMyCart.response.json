{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Response to method users.getMyCart", 
    "properties": {
        "error": {
            "description": "Error code", 
            "type": "number"
        }, 
        "error_text": {
            "description": "Error text", 
            "type": "string"
        }, 
        "error_url": {
            "description": "Link to error page", 
            "type": "string"
        }, 
        "response": {
            "properties": {
                "count": {
                    "description": "Count responses", 
                    "type": "number"
                }, 
                "items": {
                    "description": "List of responses", 
                    "items": {
                        "properties": {
                            "count": {
                                "type": "integer"
                            }, 
                            "product": {
                                "$ref": "objects.json#/definitions/products/product", 
                                "type": "object"
                            }
                        }, 
                        "required": [
                            "product", 
                            "count"
                        ], 
                        "type": "object"
                    }, 
                    "minItems": 1, 
                    "type": "array", 
                    "uniqueItems": true
                }
            }, 
            "required": [
                "count", 
                "items"
            ], 
            "type": "object"
        }, 
        "run_time": {
            "description": "Run time", 
            "type": "number"
        }, 
        "server_time": {
            "description": "Server time (unix)", 
            "type": "number"
        }
    }, 
    "required": [
        "response", 
        "run_time", 
        "server_time", 
        "error", 
        "error_text", 
        "error_url"
    ], 
    "type": "object"
}