{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Метод для получения товаров определённого магазина",
    "properties": {
        "filters": {
            "description": "Фильтры",
            "properties": {
                "latitude": {
                    "description": "Широта",
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ]
                }, 
                "longitude": {
                    "description": "Долгота",
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ]
                }
            }, 
            "type": "object"
        }, 
        "id": {
            "description": "Идентификатор магазина",
            "minNum": 1, 
            "type": "integer"
        }, 
        "last_id": {
            "description": "Последний идентификатор",
            "type": "string"
        }, 
        "limit": {
            "default": 20, 
            "description": "Лимит",
            "minNum": 1, 
            "type": "integer"
        }
    }, 
    "required": [
        "id"
    ], 
    "type": "object"
}