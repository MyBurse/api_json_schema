{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "description": "Ответ на метод auth.login",
    "properties": {
        "access_token": {
            "description": "Токен для дальнейших запросов",
            "type": "string"
        },
        "error": {
            "description": "Код ошибки",
            "type": "number"
        },
        "error_text": {
            "description": "Текст ошибки",
            "type": "string"
        },
        "error_url": {
            "description": "Ссылка на страницу ошибки",
            "type": "string"
        },
        "expire": {
            "description": "Время жизни токена",
            "type": "number"
        }
    },
    "required": [
        "access_token",
        "expire",
        "error",
        "error_text",
        "error_url"
    ],
    "type": "object"
}