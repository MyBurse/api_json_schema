{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Response to method shops.getAll", 
    "properties": {
        "error": {
            "description": "Error code", 
            "type": "number"
        }, 
        "error_text": {
            "description": "Error text", 
            "type": "string"
        }, 
        "error_url": {
            "description": "Link to error page", 
            "type": "string"
        }, 
        "response": {
            "properties": {
                "count": {
                    "description": "Count responses", 
                    "type": "number"
                }, 
                "items": {
                    "description": "List of responses", 
                    "items": {
                        "$ref": "objects.json#/definitions/shops/shop", 
                        "type": "object"
                    }, 
                    "minItems": 0, 
                    "type": "array"
                }, 
                "last_id": {
                    "description": "Last ID",
                    "type": ["string", "integer", "null"]
                }
            }, 
            "required": [
                "last_id", 
                "count", 
                "items"
            ], 
            "type": "object"
        }, 
        "run_time": {
            "description": "Run time", 
            "type": "number"
        }, 
        "server_time": {
            "description": "Server time (unix)", 
            "type": "number"
        }
    }, 
    "required": [
        "response", 
        "run_time", 
        "server_time", 
        "error", 
        "error_text", 
        "error_url"
    ], 
    "type": "object"
}