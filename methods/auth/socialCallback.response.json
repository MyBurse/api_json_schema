{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "additionalProperties": false, 
    "description": "Ответ на метод auth.socialCallback",
    "properties": {
        "access_token": {
            "description": "Токен для дальнейших запросов",
            "type": "string"
        }, 
        "error": {
            "description": "Код ошибки",
            "type": "number"
        }, 
        "error_text": {
            "description": "\u0422\u0435\u043a\u0441\u0442 \u043e\u0448\u0438\u0431\u043a\u0438", 
            "type": "string"
        }, 
        "error_url": {
            "description": "Ссылка на страницу ошибки",
            "type": "string"
        }, 
        "expire": {
            "description": "Время жизни токена",
            "type": "number"
        }
    }, 
    "required": [
        "access_token", 
        "expire", 
        "error", 
        "error_text", 
        "error_url"
    ], 
    "type": "object"
}