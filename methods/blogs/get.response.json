{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "required": [
        "response", 
        "run_time", 
        "server_time", 
        "error", 
        "error_text", 
        "error_url"
    ], 
    "type": "object", 
    "description": "Response to blogs.get", 
    "properties": {
        "server_time": {
            "type": "number", 
            "description": "Server time (unix)"
        }, 
        "error_url": {
            "type": "string", 
            "description": "Link to error page"
        }, 
        "run_time": {
            "type": "number", 
            "description": "Run time"
        }, 
        "error": {
            "type": "number", 
            "description": "Error code"
        }, 
        "response": {
            "required": [
                "last_id", 
                "count", 
                "items"
            ], 
            "type": "object", 
            "properties": {
                "count": {
                    "type": "number", 
                    "description": "Count responses"
                }, 
                "items": {
                    "items": {
                        "type": "object", 
                        "$ref": "objects.json#/definitions/blogs/blog_post"
                    }, 
                    "type": "array", 
                    "description": "List of responses"
                }, 
                "last_id": {
                    "type": [
                        "string", 
                        "integer"
                    ], 
                    "description": "Last ID"
                }
            }
        }, 
        "error_text": {
            "type": "string", 
            "description": "Error text"
        }
    }
}