{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "additionalProperties": false, 
    "description": "Response to auth.confirmSms method", 
    "properties": {
        "access_token": {
            "description": "Token for other requests", 
            "type": "string"
        }, 
        "error": {
            "description": "Error code", 
            "type": "number"
        }, 
        "error_text": {
            "description": "Error text", 
            "type": "string"
        }, 
        "error_url": {
            "description": "Link to error page", 
            "type": "string"
        }, 
        "expire": {
            "description": "Token expire", 
            "type": "number"
        }
    }, 
    "required": [
        "access_token", 
        "expire", 
        "error", 
        "error_text", 
        "error_url"
    ], 
    "type": "object"
}