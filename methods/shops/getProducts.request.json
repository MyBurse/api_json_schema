{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Method to get shop products", 
    "properties": {
        "filters": {
            "description": "Filters", 
            "properties": {
                "latitude": {
                    "description": "Latitude", 
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ]
                }, 
                "longitude": {
                    "description": "Longitude", 
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ]
                }
            }, 
            "type": "object"
        }, 
        "id": {
            "description": "Shop ID", 
            "minNum": 1, 
            "type": "integer"
        }, 
        "last_id": {
            "description": "Last ID",
            "type": "string"
        }, 
        "limit": {
            "default": 20, 
            "description": "Limit", 
            "minNum": 1, 
            "type": "integer"
        }
    }, 
    "required": [
        "id"
    ], 
    "type": "object"
}