{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Response to method global.Search",
  "properties": {
	"error": {
	  "description": "Error code",
	  "type": "number"
	},
	"error_text": {
	  "description": "Error text",
	  "type": "string"
	},
	"error_url": {
	  "description": "Link to error page",
	  "type": "string"
	},
	"response": {
	  "properties": {
		"count": {
		  "description": "Count responses",
		  "type": "number"
		},
		"items": {
		  "description": "List of responses",
		  "properties": {
			"shops": {
			  "description": "First resultes from search",
			  "type": "object",
			  "properties": {
				"data": {
				  "type": "array",
				  "items": {
					"maxItems": 5,
					"$ref": "objects.json#/definitions/shops/shop_light",
					"type": "object"
				  }
				},
				"web_url": {
				  "type": "string"
				},
				"api_url": {
				  "type": "string"
				}
			  }
			},
			"products": {
			  "type": "object",
			  "properties": {
				"data": {
				  "type": "array",
				  "description": "First resultes from search",
				  "items": {
					"maxItems": 5,
					"$ref": "objects.json#/definitions/products/product_light",
					"type": "object"
				  },
				  "web_url": {
					"type": "string"
				  },
				  "api_url": {
					"type": "string"
				  }
				}
			  }
			},
			"users": {
			  "type": "object",
			  "properties": {
				"data": {
				  "type": "array",
				  "description": "First resultes from search",
				  "items": {
					"maxItems": 5,
					"$ref": "objects.json#/definitions/users/user_light",
					"type": "object"
				  },
				  "web_url": {
					"type": "string"
				  },
				  "api_url": {
					"type": "string"
				  }
				}
			  }
			}
		  },
		  "type": "object"
		}
	  },
	  "required": [
		"count",
		"items"
	  ],
	  "type": "object"
	},
	"run_time": {
	  "description": "Run time",
	  "type": "number"
	},
	"server_time": {
	  "description": "Server time (unix)",
	  "type": "number"
	}
  },
  "required": [
	"response",
	"run_time",
	"server_time",
	"error",
	"error_text",
	"error_url"
  ],
  "type": "object"
}