{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Method to get shops list", 
    "properties": {
        "filters": {
            "description": "Filters", 
            "properties": {
                "tab": {
                    "description": "Selected tab",
                    "type": "string",
                    "enum": ["my", "subscribe", "interest", ""]
                }, 
                "geo": {
                    "description": "Geolocation filter as GeoType_GeoId",
                    "type": "string"
                }, 
                "latitude": {
                    "description": "Latitude", 
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ]
                }, 
                "longitude": {
                    "description": "lLongitude", 
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ]
                },
                "rating": {
                    "description": "Rating filter >=",
                    "minNum": 1,
                    "type": [
                        "number",
                        "null"
                    ]
                },
                "q": {
                    "description": "Search query",
                    "type": "string"
                }
            }, 
            "type": "object"
        }, 
        "last_id": {
            "description": "Last ID", 
            "type": "string"
        }, 
        "limit": {
            "description": "Limit", 
            "minNum": 1, 
            "type": "integer"
        }
    }, 
    "type": "object"
}