{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "required": [
        "id"
    ], 
    "type": "object", 
    "description": "Method to get shop products", 
    "properties": {
        "limit": {
            "default": 20, 
            "minNum": 1, 
            "type": "integer", 
            "description": "Limit"
        }, 
        "id": {
            "minNum": 1, 
            "type": "integer", 
            "description": "Shop ID"
        }, 
        "filters": {
            "type": "object", 
            "description": "Filters", 
            "properties": {
                "latitude": {
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ], 
                    "description": "Latitude"
                }, 
                "longitude": {
                    "minNum": 1, 
                    "type": [
                        "number", 
                        "null"
                    ], 
                    "description": "Longitude"
                }
            }
        }, 
        "last_id": {
            "type": "string", 
            "description": "Last ID"
        }
    }
}